        <script src="@routes.Assets.at("javascripts/hello.js")" type="text/javascript"></script>
        <script>
        function init() {
            var planet = document.getElementById("greenplanet");
            planet.innerHTML = "Red Alert."
        }
        
        window.onload = init;
        
    </head>
    <body>
		<ul> 
		@for(order <- orders) {
		  <li>@order</li>
		} 
		</ul>
		
